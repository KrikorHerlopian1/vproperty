<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingStart="10dp"
    android:paddingEnd="10dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/brick"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/brick"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/cement"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/cement"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/composition"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/composition"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/metal"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/metal"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/shingle"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/shingle"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/stone"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/stone"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/stucco"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/stucco"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/vinyl"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/vinyl"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/wood"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/wood"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/wood_products"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/wood_products"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
            android:id="@+id/other"
            android:layout_width="0dp"
            android:layout_height="@dimen/checkboxsize"
            android:layout_weight="1"
            android:text="@string/other"
            android:textColor="@color/colorAccent"
            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
    </LinearLayout>
</LinearLayout>