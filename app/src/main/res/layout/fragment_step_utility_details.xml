<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rootLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">


    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent">

                <edu.newhaven.krikorherlopian.android.vproperty.textview.BoldTextView
                    android:id="@+id/lv_list_hdr1"
                    style="@style/listViewHeaderItem"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:gravity="center|start"
                    android:paddingTop="10dp"
                    android:paddingBottom="5dp"
                    android:text="@string/cooling_type"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/lvDividerHeight"
                    android:layout_below="@id/lv_list_hdr1"
                    android:background="@color/lvHeaderDividerColor" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bordercheckbox"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/central"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/central"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/evaporative"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/evaporative"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/geothermal_cooling"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/geothermal"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/refrigeration"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/refrigeration"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/solar"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/solar"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/wall_cooling"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/wall"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/other_cooling"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/other"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/none"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/none"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent">

                <edu.newhaven.krikorherlopian.android.vproperty.textview.BoldTextView
                    android:id="@+id/lv_list_hdr"
                    style="@style/listViewHeaderItem"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:gravity="center|start"
                    android:paddingTop="10dp"
                    android:paddingBottom="5dp"
                    android:text="@string/heating_type"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/item_separator"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/lvDividerHeight"
                    android:layout_below="@id/lv_list_hdr"
                    android:background="@color/lvHeaderDividerColor" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bordercheckbox"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/baseboard"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/baseboard"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/forced_air"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/forced_air"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/geothermal"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/geothermal"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/heat_pump"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/heat_pump"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/radiant"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/radiant"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/stove"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/stove"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/wall"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/wall"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>

                        <edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox
                            android:id="@+id/other"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/checkboxsize"
                            android:layout_weight="1"
                            android:text="@string/other"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"></edu.newhaven.krikorherlopian.android.vproperty.checkbox.MyCustomCheckBox>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>